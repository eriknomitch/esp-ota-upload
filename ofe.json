{
  "default": {
    "extensions": [".ino", ".ini", ".h", "*.md"],
    "files": ["run", "upload"],
    "first_file": "src/Main.ino"
  },
  "docs": {
    "extensions": [".md"],
    "first_file": "README.md"
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore"]
  }
}
